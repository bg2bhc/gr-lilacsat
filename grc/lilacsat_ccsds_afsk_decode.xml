<block>
  <name>CCSDS AFSK Decode</name>
  <key>lilacsat_ccsds_afsk_decode</key>
  <category>[LilacSat]</category>
  <import>import lilacsat</import>
  <make>lilacsat.ccsds_afsk_decode($bitrate, $frame_len, $using_m, $using_convolutional_code, $pass_all)</make>
  <param>
    <name>Bit Rate</name>
    <key>bitrate</key>
    <value>2400</value>
    <type>int</type>
    <option>
	<name>1200 baud</name>
	<key>1200</key>
    </option>
    <option>
	<name>2400 (fc = 1800)</name>
	<key>2400</key>
    </option>
    <option>
	<name>2400 (fc = 1650)</name>
	<key>24001</key>
    </option>
  </param>
  <param>
    <name>Frame length</name>
    <key>frame_len</key>
    <value>256</value>
    <type>int</type>
  </param>
  <param>
    <name>Using M</name>
    <key>using_m</key>
    <value>False</value>
    <type>bool</type>
    <option>
	<name>On</name>
	<key>True</key>
    </option>
    <option>
	<name>Off</name>
	<key>False</key>
    </option>
  </param>
  <param>
    <name>Using convolutional code</name>
    <key>using_convolutional_code</key>
    <value>False</value>
    <type>bool</type>
    <option>
	<name>On</name>
	<key>True</key>
    </option>
    <option>
	<name>Off</name>
	<key>False</key>
    </option>
  </param>
  <param>
    <name>Pass all</name>
    <key>pass_all</key>
    <value>False</value>
    <type>bool</type>
    <option>
	<name>On</name>
	<key>True</key>
    </option>
    <option>
	<name>Off</name>
	<key>False</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
