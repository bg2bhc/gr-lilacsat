<block>
  <name>AFSK1200 TX</name>
  <key>lilacsat_afsk1200_tx_f</key>
  <category>LilacSat</category>
  <import>import lilacsat</import>
  <make>lilacsat.afsk1200_tx_f($destination, $source, $repeater1, $repeater2, $padding_zero)</make>
  <param>
    <name>Destination</name>
    <key>destination</key>
    <value>BY2HIT</value>
    <type>string</type>
  </param>
  <param>
    <name>Source</name>
    <key>source</key>
    <value>BG2BHC</value>
    <type>string</type>
  </param>
  <param>
    <name>Repeater1</name>
    <key>repeater1</key>
    <type>string</type>
  </param>
  <param>
    <name>Repeater2</name>
    <key>repeater2</key>
    <type>string</type>
  </param>
  <param>
    <name>Padding Zero</name>
    <key>padding_zero</key>
    <value>False</value>
    <type>bool</type>
    <hide>#if str($padding_zero) == 'False' then 'part' else 'none'#</hide>
    <option>
	<name>On</name>
	<key>True</key>
    </option>
    <option>
	<name>Off</name>
	<key>False</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>out</name>
    <type>float</type>
  </source>
  <source>
    <name>ptt</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
